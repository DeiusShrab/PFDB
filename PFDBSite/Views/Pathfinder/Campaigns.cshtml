@model CampaignsModel
@{
  ViewData["Title"] = "List of Campaigns";
}

<div class="jumbotron">
  <h2>List of Campaigns</h2>
</div>

<div class="row">
  <div class="span10">
    <ul>
      @foreach (var campaign in Model.CampaignList)
      {
        if (campaign.Key > 0)
        {
          <li>@Html.ActionLink(campaign.Value, "CampaignLiveDisplay", "Pathfinder", new { campaignId = campaign.Key })</li>
        }
        else
        {
          <li>@Html.ActionLink(campaign.Value, "Index", "Pathfinder")</li>
        }
      }
    </ul>
  </div>
</div>

